{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "snap",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "SYMBOL",
          "name": "body"
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tripleDash"
        },
        {
          "type": "SYMBOL",
          "name": "headerContent"
        },
        {
          "type": "SYMBOL",
          "name": "tripleDash"
        }
      ]
    },
    "tripleDash": {
      "type": "STRING",
      "value": "---\n"
    },
    "headerContent": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "headerSource"
        },
        {
          "type": "SYMBOL",
          "name": "headerExpression"
        }
      ]
    },
    "headerSource": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "source: "
        },
        {
          "type": "SYMBOL",
          "name": "restOfLine"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "headerExpression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "expression: "
        },
        {
          "type": "SYMBOL",
          "name": "restOfLine"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "restOfLine": {
      "type": "PATTERN",
      "value": "([^\n]+)"
    },
    "body": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "(.)"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}