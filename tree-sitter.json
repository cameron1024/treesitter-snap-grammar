{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "snap",
      "camelcase": "Snap",
      "title": "cargo-insta snapshot",
      "scope": "source.snap",
      "file-types": [
        "snap"
      ],
      "injection-regex": "^snap$",
      "class-name": "TreeSitterSnap"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Snapshot format for the cargo-insta snapshot testing library",
    "authors": [
      {
        "name": "Cameron",
        "email": "cameron.studdstreet@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-snap"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}